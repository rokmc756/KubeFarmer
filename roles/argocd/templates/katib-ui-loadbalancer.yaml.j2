---
apiVersion: v1
kind: Service
metadata:
  name: katib-ui
  namespace: jenkins
  annotations:
    nginx.ingress.kubernetes.io/proxy-protocol: "true"
spec:
  selector:
    app: katib-ui
  ports:
    - name: http
      protocol: TCP
      port: 80
      targetPort: 8080
  type: LoadBalancer

---
apiVersion: v1
kind: Service
metadata:
  name: centraldashboard
  namespace: jenkins
  annotations:
    nginx.ingress.kubernetes.io/proxy-protocol: "true"
spec:
  selector:
    app: centraldashboard
  ports:
    - name: http
      protocol: TCP
      port: 8080
      targetPort: 8080
  type: LoadBalancer

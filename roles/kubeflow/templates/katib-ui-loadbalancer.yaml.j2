---
apiVersion: v1
kind: Service
metadata:
  name: katib-ui
  namespace: kubeflow
  annotations:
    nginx.ingress.kubernetes.io/proxy-protocol: "true"
spec:
  ports:
    - name: http
      protocol: TCP
      port: 80
      targetPort: 8080
  type: LoadBalancer
  loadBalancerIP: 192.168.1.219

